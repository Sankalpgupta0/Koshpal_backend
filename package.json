{
  "name": "koshpal-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "prisma": {
    "seed": "ts-node prisma/seed/seed.ts"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "dev": "concurrently --kill-others-on-fail --prefix \"[{name}]\" --names \"SERVER,UPLOAD-WORKER,INSIGHTS-WORKER,CONSULTATION-WORKER\" --prefix-colors \"cyan,magenta,yellow,green\" \"npm:start:dev\" \"npm:worker:dev\" \"npm:insights-worker:dev\" \"npm:consultation-worker:dev\"",
    "start:all": "concurrently --kill-others-on-fail --prefix \"[{name}]\" --names \"SERVER,UPLOAD-WORKER,INSIGHTS-WORKER,CONSULTATION-WORKER\" --prefix-colors \"cyan,magenta,yellow,green\" \"npm:start:prod\" \"npm:worker:prod\" \"npm:insights-worker:prod\" \"npm:consultation-worker:prod\"",
    "start:pm2": "pm2 start ecosystem.config.js",
    "start:pm2:dev": "pm2 start ecosystem.config.js --env development",
    "stop:pm2": "pm2 stop ecosystem.config.js",
    "restart:pm2": "pm2 restart ecosystem.config.js",
    "delete:pm2": "pm2 delete ecosystem.config.js",
    "logs:pm2": "pm2 logs",
    "monit:pm2": "pm2 monit",
    "worker:dev": "ts-node src/workers/employee-upload.worker.ts",
    "worker:prod": "node dist/workers/employee-upload.worker.js",
    "insights-worker:dev": "ts-node src/workers/monthly-insights.worker.ts",
    "insights-worker:prod": "node dist/workers/monthly-insights.worker.js",
    "consultation-worker:dev": "ts-node src/workers/consultation-mail.worker.ts",
    "consultation-worker:prod": "node dist/workers/consultation-mail.worker.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "prisma db seed"
  },
  "dependencies": {
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/throttler": "^6.5.0",
    "@prisma/client": "^6.19.1",
    "@types/cookie-parser": "^1.4.10",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.66.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cloudinary": "^1.41.3",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "csurf": "^1.11.0",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.3",
    "googleapis": "^169.0.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^7.0.11",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^6.19.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.19.3",
    "@types/nodemailer": "^7.0.4",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "concurrently": "^9.2.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
